@model svg.ViewModels.SvgObjectVew

<div class="thumbnail">
    <img src="@Url.Action("Thumbnail", new { id = Model.Id })" alt="thumbnail" />
    <div class="caption">
        <h3>@Model.Name</h3>
        <p>
            <a class="btn btn-default" href="@Url.Action("FullImage", new { id = Model.Id })"></a>
            <a class="btn btn-default" href="@Url.Action("Delete", new { id = Model.Id })"></a>
        </p>
    </div>
</div>
